{
  "version": "0.2",
  "language": "en",
  "words": [],
  "dictionaries": ["custom"],
  "dictionaryDefinitions": [
    {
      "name": "custom",
      "path": "./.cspell/custom-dictionary.txt",
      "addWords": true
    }
  ],
  "ignorePaths": [
    "node_modules",
    ".git",
    "*.lock",
    "*.json",
    "*.yaml",
    "*.yml",
    ".gitignore",
    "deployment-files/**",
    "instance/**",
    "tests/**",
    "__pycache__",
    "dist",
    "build",
    "*.egg-info",
    ".aws_cache",
    "instances.json",
    ".secrets.baseline"
  ],
  "ignoreRegExpList": [
    "/[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}/gi",
    "/i-[0-9a-f]{17}/gi",
    "/ami-[0-9a-f]+/gi"
  ]
}
